#include <stdio.h>
#include <stdlib.h>

//determine the largest of the two numbers.

int main()
{
    int a, b, largest;
    printf("\nEnter the first number: ");
    scanf("%d", &a);

    printf("\nEnter the second number: ");
    scanf("%d", &b);

    (a>b)?(largest = a):(largest = b);
    printf("\nThe largest number out of the two numbers you entered is : %d\n", largest);
}
