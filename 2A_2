#include <stdio.h>
#include <stdlib.h>
//Determine whether the year is a leap or not
//PROGRAM:
//Check whether the year is leap or not*/
//Year is leap if it is a century year and is divisible by 400*/
//Year is Leap if it a non-century year and is divisible by 4
int main()
{
    int yr;
    printf("\nEnter a year: ");
    scanf("%d", &yr);

    if(yr%100==0)
    {
        if(yr%400==0)
            printf("Leap year\n");
        else
            printf("Not a Leap year\n");
    }
    else
    {
        if(yr%4==0)
            printf("Leap year\n");
        else
            printf("Not a Leap Year\n");
    }
}
