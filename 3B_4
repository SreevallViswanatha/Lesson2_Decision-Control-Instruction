#include <stdio.h>
#include <stdlib.h>
//determine the largest and change each to largest
int main()
{

    int a, b;

    printf("Enter the value of a: ");
    scanf("%d", &a);

    printf("Enter the value of b: ");
    scanf("%d", &b);

    if (a>b)
        b=a;
    else
        a=b;

    printf("The value of a = %d\n", a);
    printf("The value of b = %d\n", b);
}
