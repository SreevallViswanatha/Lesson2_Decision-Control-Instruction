#include <stdio.h>
#include <stdlib.h>

//Determine Whether a year is a leap year or not using conditional operators.
int main()
{
    int year;

    printf("Enter a year: ");
    scanf("%d", &year);
    year%100 == 0?(year%400==0?printf("Leap Year\n"):
                   printf("Not a Leap Year\n")): (year%4 == 0?
                   printf("Leap Year\n"):printf("Not a Leap Year\n"));
}
