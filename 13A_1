#include <stdio.h>
#include <stdlib.h>
//Determine whether a year is leap or not.
int main()
{
    int year;

    printf("Enter year: ");
    scanf("%d", &year);

    if(year%400 == 0 || year%100 != 0 && year%4 == 0)
        printf("The year you entered - (%d) is a Leap year\n", year);
    else
        printf("The year you entered - (%d) is not a Leap year.\n", year);
}
