#include <stdio.h>
#include <stdlib.h>

//Determine character is a special symbol
int main()
{
    char ch;

    printf("Enter Character: ");
    scanf("%c", &ch);
    ((ch>=0 && ch<=47)||(ch>=58 && ch<=64)||(ch>=91 &&ch<=96)||(ch>=123))?
        printf("Character entered is a special symbol\n"):
        printf("Character entered is not a special symbol\n");
}
